{
  "name": "automatizacao_cnc",
  "displayName": "automatizacao_cnc",
  "description": "Extensão para inserir cabeçalho e ciclos CNC no VS Code.",
  "version": "0.0.1",
  "publisher": "Anna Renata Da Silva Menezes",
  "icon": "img/logo.jpg",
  "engines": {
    "vscode": "^1.90.0"
  },
  "license":"MIT",
  "homepage": "https://github.com/AnnaSilvaaa/Projeto-CNC",
  "repository":{
     "type":"git",
     "url":"https://github.com/AnnaSilvaaa/Projeto-CNC.git"
  },
   "keywords": [
    "Programacao cnc",
    "linguagem cnc",
    "Cabeçalho da fresa",
    "Ciclos de torneamento"
   ],

  "categories": [
    "Other"
  ],
  "activationEvents": [
   
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.insertCustomHeader",
        "title": "Inserir Cabeçalho Personalizado"
      },
      {
        "command": "extension.insertCycle",
        "title": "Inserir Ciclo"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "extension.insertCustomHeader",
          "when": "editorTextFocus && !extension.hasHeader"
        },
        {
          "command": "extension.insertCycle",
          "when": "editorTextFocus"
        }
        
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.90.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "ts-loader": "^9.5.1",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}
